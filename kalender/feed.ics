DTEND;TZID=/Europe/Copenhagen:{{ event.date_end | replace: '-', '' }}T{{ event.time_end | replace: ':', '' }}00
DESCRIPTION:Mere info findes på hjemmesiden: https://museum.soeborggruppe.dk{{ event.url }}
URL;VALUE=URI:https://museum.soeborggruppe.dk{{ event.url }}
END:VEVENT
{% endfor %}

END:VCALENDAR
