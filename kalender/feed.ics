BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//soeborggruppe.dk//SpejderApp ICAL//DK
CALSCALE:GREGORIAN
METHOD:PUBLISH
X-WR-CALNAME: Kalender | 1. Søborg Gruppe

{% for event in site.events %}
BEGIN:VEVENT
SUMMARY:{{ event.title }}
